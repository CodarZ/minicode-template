{
  "name": "minicode-native-template",
  "version": "0.0.1",
  "description": "微信小程序 native 模板",
  "author": "CoderZ <1476712061@qq.com>",
  "license": "MIT",
  "keywords": [
    "微信小程序",
    "原生",
    "TDesign",
    "skyline"
  ],
  "engines": {
    "node": ">=20.6.1"
  },
  "scripts": {
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "lint:prettier": "prettier --write \"**/*.{js,ts,wxs,json,css,wxss,html,wxml,md}\"",
    "lint:eslint": "eslint .",
    "lint:style": "stylelint \"src/**/*.{css,scss}\"",
    "lint:fix": "eslint . --fix && stylelint --fix \"src/**/*.{css,wxss,scss}\" && prettier --write \"**/*.{js,ts,wxs,json,css,wxss,html,wxml,md}\"",
    "type-check": "tsc"
  },
  "dependencies": {
    "dayjs": "^1.11.13",
    "tdesign-miniprogram": "^1.8.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.2.0",
    "conventional-changelog-cli": "^5.0.0",
    "eslint": "^9.20.1",
    "eslint-config-prettier": "^10.0.1",
    "globals": "^15.14.0",
    "miniprogram-api-typings": "^4.0.5",
    "prettier": "^3.5.0",
    "standard-changelog": "^6.0.0",
    "typescript": "^5.7.3"
  }
}
