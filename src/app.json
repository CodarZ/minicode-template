{
  "entryPagePath": "pages/redirect/index",
  "pages": ["pages/redirect/index", "pages/home/index", "pages/login/index"],
  "subPackages": [],
  "darkmode": false,
  "themeLocation": "theme.json",
  "window": {
    "navigationBarTitleText": "Minicode",
    "navigationStyle": "custom",
    "backgroundColor": "@bgColor",
    "backgroundColorTop": "@bgColor",
    "navigationBarTextStyle": "@style",
    "backgroundColorBottom": "@bgColor",
    "navigationBarBackgroundColor": "@bgColor"
  },
  "usingComponents": {
    "t-icon": "tdesign-miniprogram/icon/icon",
    "t-navbar": "tdesign-miniprogram/navbar/navbar",
    "t-toast": "tdesign-miniprogram/toast/toast",
    "t-button": "tdesign-miniprogram/button/button"
  },
  "useExtendedLib": {
    "weui": true
  },
  "resolveAlias": {
    "@/*": "/*"
  },
  "workers": {
    "path": "workers",
    "isSubpackage": false
  },
  "renderer": "skyline",
  "componentFramework": "glass-easel",
  "rendererOptions": {
    "skyline": {
      "defaultDisplayBlock": true,
      "defaultContentBox": true,
      "enableScrollViewAutoSize": true,
      "tagNameStyleIsolation": "legacy",
      "disableABTest": true,
      "sdkVersionBegin": "3.5.8",
      "sdkVersionEnd": "255.255.255"
    }
  },
  "sitemapLocation": "sitemap.json",
  "lazyCodeLoading": "requiredComponents"
}
